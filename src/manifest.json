{
    "manifest_version": 3,
    "name": "Clear Links",
    "version": "4.1.1",
	"author": "Christopher Bull",
    "description": "Makes website links more transparent and accessible, enhancing your awareness of your navigational security and privacy.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
	"permissions": [
		"scripting",
		"storage",
		"webNavigation"
	],
	"host_permissions": [
		"http://*/",
        "https://*/"
	],
	"key": "",
	"web_accessible_resources": [
		{
		  "resources": [
			"images/*",
			"contentScript.css",
			"contentScript.js",
			"contentScriptSharedLib.js",
			"defaultSettings.js"
		  ],
		  "matches": [
			"<all_urls>"
		  ]
		}
	],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"options_ui": {
		"page": "options.html"
	},
	"content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": [
				"./lib/browser-polyfill.min.js",
				"contentScriptActivationFilter.js"
			],
			"run_at": "document_start"
        }
    ]
}
